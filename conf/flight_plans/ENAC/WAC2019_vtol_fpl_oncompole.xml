<!DOCTYPE flight_plan SYSTEM "../flight_plan.dtd">

<flight_plan alt="182" ground_alt="152" lat0="43.6132601" lon0="1.4000453" max_dist_from_home="8000" name="WAC2019_vtol_fpl" security_height="25">
  <header>
#include "autopilot.h"
  </header>
  <waypoints>
    <waypoint name="HOME" x="0.0" y="0.0"/>
    <waypoint name="CLIMB" x="0.0" y="0.0"/>
    <waypoint lat="43.6133114" lon="1.4003364" name="STDBY"/>
